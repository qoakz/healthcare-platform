{
  "version": 2,
  "builds": [
    {
      "src": "app/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@api_url",
    "NEXT_PUBLIC_COGNITO_DOMAIN": "@cognito_domain",
    "NEXT_PUBLIC_COGNITO_CLIENT_ID": "@cognito_client_id"
  }
}
